{
  "name": "droptags",
  "version": "0.0.0",
  "description": "A React/Flux app generate by RF, powered with coffee-script",
  "main": "build/bundle.js",
  "dependencies": {
    "lodash": "^3.3.1",
    "object-assign": "^2.0.0",
    "react": "^0.12.2",
    "react-router-component": "^0.23.2",
    "reflux": "^0.2.5",
    "store": "^1.3.17",
    "superagent": "^0.21.0"
  },
  "devDependencies": {
    "css-loader": "*",
    "file-loader": "*",
    "jest-cli": "*",
    "react-hot-loader": "^1.1.4",
    "react-tools": "^0.12.2",
    "style-loader": "*",
    "webpack-dev-server": "^1.7.0",
    "webpack": "^1.4.14",
    "cjsx-loader": "^1.1.0",
    "coffee-loader": "*",
    "coffee-script": "*",
    "sass-loader": "*"
  },
  "scripts": {
    "build": "webpack",
    "dev": "webpack-dev-server --colors --content-base ./build",
    "test": "jest"
  },
  "author": "",
  "license": "MIT",
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "testFileExtensions": [
      "js",
      "coffee"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "coffee"
    ],
    "globals": {
      "SCRIPTS": "<rootDir>/src/scripts"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/node_modules/"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  }
}
